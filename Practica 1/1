program xx;

var
num1,num2: integer;

begin
	writeln('Ingrese el primer numero');
	readln(num1);
	writeln('Ingrese el segundo numero');
	readln(num2);
	if (num1 > num2) then	
		writeln('El numero ',num1,' es mayo a ',num2)
	else if (num1 < num2) then 
		writeln('El numero ',num2,' es mayo a ',num1)
	else
		writeln('Los numeos son iguales');
end.
